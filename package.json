{
  "name": "demo",
  "displayName": "demo",
  "description": "demo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.showOptions",
        "title": "Show Options"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.showOptions",
          "when": "editorLangId == java",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "testableMethods",
          "title": "Testable Methods",
          "icon": "resources/logo.png"
        }
      ]
    },
    "views": {
      "testableMethods": [
        {
          "id": "testableMethodsView",
          "name": "Methods"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^4.0.3",
    "vscode": "^1.1.36"
  }
}
